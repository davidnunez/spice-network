# Classes
food:Spice a rdfs:Class ; rdfs:label "Spice" .
food:Blend a rdfs:Class ; rdfs:label "Blend" .

# Properties (light schema)
food:flavorProfile a rdf:Property ; rdfs:label "flavor profile" .
food:cuisine a rdf:Property ; rdfs:label "cuisine" .
food:pairsWellWith a rdf:Property ; rdfs:label "pairs well with" ; rdfs:domain food:Spice ; rdfs:range food:Spice .
food:category a rdf:Property ; rdfs:label "category" .
food:plantPart a rdf:Property ; rdfs:label "plant part used" .
food:originRegion a rdf:Property ; rdfs:label "origin/region" .
food:form a rdf:Property ; rdfs:label "common form" .
food:processing a rdf:Property ; rdfs:label "processing" .
food:alias a rdf:Property ; rdfs:label "alias/common name" .
food:heatScoville a rdf:Property ; rdfs:label "heat (Scoville SHU)" ; rdfs:range xsd:integer .
food:signatureDish a rdf:Property ; rdfs:label "signature dish/example" .
food:substitutes a rdf:Property ; rdfs:label "substitutes" ; rdfs:domain food:Spice ; rdfs:range food:Spice .

